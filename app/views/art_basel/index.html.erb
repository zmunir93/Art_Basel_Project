<h1 id="head">Events</h1>

<% @entriesArray.each do |entry| %>
  <div class="event">
  	<!-- <div class="event-wrapper"> -->
		<a href="<%= entry.link %>">
		  	<div class="image"><img src="<%= entry.image %>"></div>
		  		<div class="text-wrapper">
				  	<h3 class="title"><%= entry.title %></h3>
				  	<h5 class="date"><%= entry.date %></h5>
				  	<div class="venue"><%= entry.venue %></div>
				  		
			  	</div>
  	<!-- </div> -->
		</a>
  </div>


<% end %>



